<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:local="clr-namespace:WpfCommonStyles"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="../Skins/SimpleElements.xaml"/>
    </ResourceDictionary.MergedDictionaries>
    <Style x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type local:CustomResources}, ResourceId=TitleStyle}" BasedOn="{StaticResource TitleStyle}"/>
</ResourceDictionary>